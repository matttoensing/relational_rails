<%= form_tag("/libraries", method: "post") do %>
<%= label_tag  "name_id", "Name" %>
<%= text_field_tag(:name) %> <br>
<%= label_tag  "public_id", "Public" %>
<%= text_field_tag(:public) %> <br>
<%= label_tag  "zip_code", "Zip Code" %>
<%= text_field_tag(:zip_code) %> <br>
<%= submit_tag("Create Library") %>
<% end %>
