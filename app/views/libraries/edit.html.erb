<h1>Edit Library</h1>

<%= form_for('library') do |f| %>

<p>
  <%= f.label :name %>
  <%= f.text_field :name %>
</p>

<p>
  <%= f.label :public %>
  <%= f.text_field :public %>
</p>

<p>
  <%= f.label :zip_code %>
  <%= f.text_field :zip_code %>
</p>

<div class="actions">
    <%= f.submit class: 'btn btn-secondary' %>
  </div>

<% end %>
