<h1>Museums</h1>
<% @museums.each do |museum| %>
<h3><%= museum.name %></h3>
<p><%= museum.free %></p>
<p><%= museum.entry_fee %></p>
<p>
  <%= link_to museum.name, "/museums/#{museum.id}" %>
</p>
<p>
  <%= link_to "Edit: #{museum.name}", "/museums/#{museum.id}/edit" %>
</p>
<p>
  <%= button_to "Delete: #{museum.name}", "/museums/#{museum.id}", method: :delete %>
</p>
<% end %>
<br>
<br>
<p><%= link_to 'Create a new museum', '/museums/new' %></p>
