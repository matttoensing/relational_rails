<%= form_tag("/libraries/#{@library.id}/members", method: "post") do %>
<%= label_tag  "fname_id", "First Name" %>
<%= text_field_tag(:first_name) %> <br>
<%= label_tag  "lname_id", "Last Name" %>
<%= text_field_tag(:last_name) %> <br>
<%= label_tag  "age_id", "Age" %>
<%= text_field_tag(:age) %> <br>
<%= label_tag  "late_fees_id", "Late Fees" %>
<%= check_box_tag(:late_fees) %> <br>
<%= submit_tag("Submit") %>
<% end %>
